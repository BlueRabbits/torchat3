    "node-notifier": "^5.4.0",
    "prop-types": "^15.7.2",
    "rc-progress": "^2.3.0",
    "react": "^16.8.6",
    "react-collapsible": "^2.6.0",
    "react-dom": "^16.8.6",
    "react-file-drop": "^0.2.8",
    "react-perfect-scrollbar": "^1.5.0",
    "react-switch": "^5.0.0",
    "sha3": "^2.0.1",
    "socks": "^2.3.2",
    "supercop.js": "^2.0.1",
    "sweetalert2": "^8.9.0",
    "sweetalert2-react-content": "^1.1.0",
    "xss": "^1.0.6",
    "xss-filters": "^1.2.7"
