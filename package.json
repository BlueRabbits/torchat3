  "build": {
    "appId": "com.kjs104901.torchat3",
    "win": {
      "target": [
        "zip"
      ],
      "icon": "./assets/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": "always",
      "installerHeaderIcon": "./assets/icon.ico"
    },
    "asar": true,
    "asarUnpack": [
      "data/**",
      "tor/bin/**"
    ]
  }
